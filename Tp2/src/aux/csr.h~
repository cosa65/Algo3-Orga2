	#include <vector>
#include <iostream>

using namespace std;

typedef vector<long double> fila;

struct MatrizE
{
public:

//generador basico, no se usa
MatrizE();

//~Matriz();

//generador que te crea una matriz de un tama√±o, no pide vectores

MatrizE( int CFilas, int Ccolumnas);

//generador que te crea la matriz con los vectores que le pasaste
MatrizE( vector< long double > valores, int cantfilas, int cantcolumnas, vector<int> posvalores, vector<int> inicioFilas);

void Definir(long double adefinir, int fila, int columna);

long double Posicion(int fila, int columna);

int Cfilas();

void mostrar();

private:

vector <long double> _valores;

vector <int> _posValores;

vector <int> _inicioFilas;

int _Cfilas;

int _Ccolumnas;

};
